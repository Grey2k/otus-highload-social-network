<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity BuildingMaterial.
    -->
    <changeSet id="20181201110718-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="building_object_id"
                                 baseTableName="building_material"
                                 constraintName="fk_building_material_building_object_id"
                                 referencedColumnNames="id"
                                 referencedTableName="building_object"/>

        <addForeignKeyConstraint baseColumnNames="vehicle_id"
                                 baseTableName="building_material"
                                 constraintName="fk_building_material_vehicle_id"
                                 referencedColumnNames="id"
                                 referencedTableName="vehicle"/>

        <addForeignKeyConstraint baseColumnNames="route_id"
                                 baseTableName="building_material"
                                 constraintName="fk_building_material_route_id"
                                 referencedColumnNames="id"
                                 referencedTableName="route"/>

        <addForeignKeyConstraint baseColumnNames="factory_id"
                                 baseTableName="building_material"
                                 constraintName="fk_building_material_factory_id"
                                 referencedColumnNames="id"
                                 referencedTableName="factory"/>

    </changeSet>
</databaseChangeLog>
